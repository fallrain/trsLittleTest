}

.hago-index-cnt-list-goods-item:after{
  content: '';
  display: block;
  clear: both;
}

.hago-index-cnt-list-goods-item-img{
  float: left;
  width: 188rpx;
  height: 200rpx;
}

.hago-index-cnt-list-goods-item-inf{
  float: left;
  width: 548rpx;
  height: 100%;
  padding-top: 36rpx;
  padding-bottom:30rpx;
  border-bottom:1px solid #ccc;
}

/*商品名称*/
.hago-index-cnt-list-goods-item-name{
  font-size:30rpx;
  color: #333;
}

.hago-index-cnt-list-goods-item-spec{
  font-size: 18rpx;
  color: #666;
}

.hago-index-cnt-list-goods-item-btm{
  margin-top: 40rpx;
}

.hago-index-cnt-list-goods-item-btm-l{
  float: left;
  font-size:22rpx;
  color: #333;
}

.hago-index-cnt-list-goods-item-price{
  font-size:24rpx;
  color: #ec0004;
}

.hago-index-cnt-list-goods-item-btm-r{
  width: 220rpx;
